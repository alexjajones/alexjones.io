{
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "react",
    "jsxFactory": "h",
    "target": "es5",
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "baseUrl": "./",
    "paths": {
      "styles/*": ["src/core/styles/"],
      "components/*": ["src/components/*"]
    }
  }
}